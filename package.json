{
  "name": "migrado",
  "version": "1.0.5",
  "description": "ArangoDB migrations and batch processing manager",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint '*/**/*.ts' --quiet --fix",
    "format": "prettier --write \"*/**/*.ts\" ",
    "prepare": "yarn build",
    "prepublishOnly": "yarn format & yarn lint",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iraycd/migrado.git"
  },
  "author": "Ray Chintalapati <i@iraycd.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iraycd/migrado/issues"
  },
  "homepage": "https://github.com/iraycd/migrado#readme",
  "dependencies": {
    "arangojs": "^7.0.1",
    "fs-extra": "^9.0.1",
    "jest": "^26.4.2",
    "klaw-sync": "^6.0.0",
    "path-ensure": "^2.0.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.3.0",
    "tslog": "^2.8.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.13",
    "@types/klaw-sync": "^6.0.0",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.1"
  },
  "keywords": [
    "ArangoDB",
    "Migration",
    "Library"
